  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch tmp",
      "program": "${workspaceFolder}/src/tmp.js",
      "outFiles": [
        "${workspaceFolder}/.compiled/**/*.js"
      ]
    }
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Apollo",
      "program": "${workspaceFolder}/src/server.js",
      "outFiles": [
        "${workspaceFolder}/.compiled/**/*.js"
      ],
      "runtimeArgs": [
        "-r",
        "dotenv/config"
      ],
      "env": {"DEBUG": "ringlord"}
    }
  ]
}
